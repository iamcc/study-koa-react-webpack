{
  "name": "section-1",
  "version": "1.0.0",
  "description": "study nodejs koa react webpack, section 1",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon index.js",
    "dev": "NODE_ENV=dev webpack-dev-server",
    "test": "npm run test-server",
    "test-cov": "npm run cov-server",
    "test-server": "NODE_ENV=test mocha -r should -R spec --recursive test/server",
    "cov-server": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- -r should -R spec --recursive test/server && cat ./coverage/lcov.info | coveralls",
    "build": "NODE_ENV=prod webpack -p",
    "lint": "eslint client server test"
  },
  "keywords": [
    "study",
    "nodejs",
    "koa",
    "react",
    "webpack"
  ],
  "author": "CC",
  "license": "ISC",
